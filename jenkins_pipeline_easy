
pipeline { 
    agent any 
    options {
        skipStagesAfterUnstable()
    }
    stages {
         stage('SCM'){
            git 'https://github.com/hehetc/meeting-server.git'
        }
      stage("build & SonarQube analysis") {
               node {
                   withSonarQubeEnv('sonarqube7.1') {
                      sh 'mvn clean package sonar:sonar'
                   }
               }
           }
     }
    
}
